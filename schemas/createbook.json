{
    "definitions": {},
    "$schema": "http://json-schema.org/draft-07/schema#",
    "$id": "http://example.com/root.json",
    "type": "object",
    "title": "The Root Schema",
    "required": [
      "isbn",
      "amazon_url",
      "author",
      "language",
      "pages",
      "publisher",
      "title",
      "year"
    ],
    "properties": {
      "isbn": {
        "$id": "#/properties/isbn",
        "type": "string",
        "title": "The Isbn Schema",
        "default": "",
        "examples": [
          "1542048273"
        ],
        "pattern": "^(.*)$"
      },
      "amazon_url": {
        "$id": "#/properties/amazon_url",
        "type": "string",
        "title": "The Amazon_url Schema",
        "default": "",
        "examples": [
          "https://www.amazon.com/Storytellers-Secret-Novel-Sejal-Badani/dp/1542048273"
        ],
        "pattern": "^(.*)$"
      },
      "author": {
        "$id": "#/properties/author",
        "type": "string",
        "title": "The Author Schema",
        "default": "",
        "examples": [
          "Sejal Badani"
        ],
        "pattern": "^(.*)$"
      },
      "language": {
        "$id": "#/properties/language",
        "type": "string",
        "title": "The Language Schema",
        "default": "",
        "examples": [
          "english"
        ],
        "pattern": "^(.*)$"
      },
      "pages": {
        "$id": "#/properties/pages",
        "type": "number",
        "multipleOf": 1,
        "minimum": 0,
        "title": "The Pages Schema",
        "default": 0,
        "examples": [
          400
        ]
      },
      "publisher": {
        "$id": "#/properties/publisher",
        "type": "string",
        "title": "The Publisher Schema",
        "default": "",
        "examples": [
          "Lake Union Publishing"
        ],
        "pattern": "^(.*)$"
      },
      "title": {
        "$id": "#/properties/title",
        "type": "string",
        "title": "The Title Schema",
        "default": "",
        "examples": [
          "The Storyteller's Secret: A Novel"
        ],
        "pattern": "^(.*)$"
      },
      "year": {
        "$id": "#/properties/year",
        "type": "integer",
        "title": "The Year Schema",
        "default": 0,
        "examples": [
          2018
        ]
      }
    }
  }